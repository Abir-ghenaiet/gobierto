<% @sdgs.sdgs_terms.each do |sdg| %>
  <% if @sdgs.sdgs_with_projects.include?(sdg) %>
    <%= link_to(
          image_tag(@sdgs.sdg_icon(sdg)),
          gobierto_plans_plan_sdg_path(slug: @plan.plan_type.slug, year: @plan.year, sdg_slug: sdg.slug),
          class:"sdg_icon"
        ) %>
  <% else %>
    <%= image_tag(@sdgs.sdg_icon(sdg), class:"sdg_icon sdg_icon_inactive") %>
  <% end %>
<% end %>
